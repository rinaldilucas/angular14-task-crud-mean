    "main": "index.js",
    "name": "mean-stack-template",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rinaldilucas/mean-stack-template.git"
    },
    "scripts": {
        "api-build": "sucrase ./api -d ./dist --transforms typescript,imports --prod=true",
        "api-debug": "nodemon -e ts,js --exec ts-node -r tsconfig-paths/register ./api/server.ts",
        "api-prebuild": "rimraf dist",
        "build": "yarn api-prebuild && yarn api-build && yarn client-build",
        "client-build": "ng build --configuration production --base-href=/mean-stack-template/",
        "client-build-debug": "ng build --configuration development --watch",
        "client-serve": "ng serve --o",
        "client-test": "ng test",
        "dev": "run-p client-serve api-debug",
        "lint": "yarn lint-client && yarn lint-api",
        "lint-api": "npx eslint ./api --ext .ts && yarn lint-types",
        "lint-types": "tsc --noemit",
        "lint-client": "ng lint && yarn lint-types && yarn lint-sass:fix && yarn lint-sass",
        "lint-sass": "sass-lint -c .sasslintrc.json -v -q",
        "lint-sass:fix": "sass-lint-auto-fix --config-sass-lint .sasslintrc.json",
        "start": "npm install && node ./api/server.ts"
    },
    "version": "1.0.0"
}
