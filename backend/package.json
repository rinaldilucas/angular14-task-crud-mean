    "typescript": "4.7.4"
  },
  "license": "MIT",
  "name": "mean-task-manager",
  "scripts": {
    "build": "babel ./ --extensions \".ts\" --out-dir dist && cp .env dist && cp swagger.json dist",
    "build:server": "babel ./ --extensions \".ts\" --out-dir dist && cp swagger.json dist",
    "debug": "nodemon --inspect --watch api --exec \"node -r sucrase/register -r tsconfig-paths/register server.ts\" -e ts,json",
    "dev": "nodemon --watch api --exec \"node -r sucrase/register -r tsconfig-paths/register server.ts\" -e ts,json",
    "format": "prettier . !dist !node_modules --log-level warn --write .",
    "lint": "yarn format &&  npx eslint ./ --fix --ext .ts && yarn lint:types",
    "lint:types": "tsc --noemit",
    "start": "node dist/server.js --prod=true"
  }
}
