    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.21.5",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/async": "^3.2.19",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.12",
