{
    "extends": "../../../tslint.json",
    "rules": {
        "directive-selector": [true, "attribute", "app", "camelCase"],
        "component-selector": [true, "element", "app", "kebab-case"],
        "max-line-length": [
            false,
            {
                "limit": 120,
                "ignore-pattern": "^import [^,]+ from |^export | implements"
            }
        ]
    }
}
